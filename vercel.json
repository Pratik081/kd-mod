{
    "version": 2,
    "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "src", "use": "@vercel/static-build" }
  ]
    ,
     "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "src": "/bot/add-theme", "dest": "/" },
    { "src": "bot/delete-theme/:title", "dest": "/" }
      
      ]
}
